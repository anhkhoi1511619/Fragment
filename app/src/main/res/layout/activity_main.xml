<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <FrameLayout
        android:id="@+id/frame_layout5"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/layout_bg"
        app:layout_constraintEnd_toStartOf="@+id/frame_layout6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frame_layout1" />

    <FrameLayout
        android:id="@+id/frame_layout7"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@drawable/layout_bg"
        app:layout_constraintBottom_toBottomOf="@+id/frame_layout6"
        app:layout_constraintEnd_toStartOf="@+id/frame_layout8"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/frame_layout6"
        app:layout_constraintTop_toTopOf="@+id/frame_layout6" />

    <FrameLayout
        android:id="@+id/frame_layout6"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@drawable/layout_bg"
        app:layout_constraintBottom_toBottomOf="@+id/frame_layout5"
        app:layout_constraintEnd_toStartOf="@+id/frame_layout7"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/frame_layout5"
        app:layout_constraintTop_toTopOf="@+id/frame_layout5" />

    <FrameLayout
        android:id="@+id/frame_layout8"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@drawable/layout_bg"
        app:layout_constraintBottom_toBottomOf="@+id/frame_layout7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/frame_layout7"
        app:layout_constraintTop_toTopOf="@+id/frame_layout7" />

    <FrameLayout
        android:id="@+id/frame_layout4"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/layout_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/frame_layout3"
        app:layout_constraintTop_toBottomOf="@+id/btn_dis_list" />

    <FrameLayout
        android:id="@+id/frame_layout1"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@drawable/layout_bg"
        app:layout_constraintBottom_toBottomOf="@+id/frame_layout2"
        app:layout_constraintEnd_toStartOf="@+id/frame_layout2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/frame_layout2" />

    <FrameLayout
        android:id="@+id/frame_layout2"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@drawable/layout_bg"
        app:layout_constraintBottom_toBottomOf="@+id/frame_layout3"
        app:layout_constraintEnd_toStartOf="@+id/frame_layout3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/frame_layout1"
        app:layout_constraintTop_toTopOf="@+id/frame_layout3" />

    <FrameLayout
        android:id="@+id/frame_layout3"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@drawable/layout_bg"
        app:layout_constraintBottom_toBottomOf="@+id/frame_layout4"
        app:layout_constraintEnd_toStartOf="@+id/frame_layout4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/frame_layout2"
        app:layout_constraintTop_toTopOf="@+id/frame_layout4" />

    <Button
        android:id="@+id/btn_dis_list"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="5dp"
        android:backgroundTint="#E91E63"
        android:text="Display List"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn_fragment5"
        app:layout_constraintTop_toBottomOf="@+id/frame_layout" />

    <Button
        android:id="@+id/btn_fragment5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="5dp"
        android:backgroundTint="#9C27B0"
        android:text="Display Fragment 5"
        app:layout_constraintBottom_toBottomOf="@+id/btn_add_total"
        app:layout_constraintEnd_toStartOf="@+id/btn_dis_list"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn_add_total"
        app:layout_constraintTop_toTopOf="@+id/btn_add_total" />

    <Button
        android:id="@+id/btn_add_total"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="5dp"
        android:layout_marginTop="16dp"
        android:text="Display All Fragment"
        app:layout_constraintBottom_toBottomOf="@+id/btn_dis_list"
        app:layout_constraintEnd_toStartOf="@+id/btn_fragment5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btn_dis_list" />

    <FrameLayout
        android:id="@+id/frame_layout"
        android:layout_width="0dp"
        android:layout_height="500dp"
        android:background="@drawable/layout_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>


    <Button
        android:id="@+id/btn_connectDB"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="@android:color/holo_purple"
        android:text="Connect DataBase"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frame_layout6" />

</androidx.constraintlayout.widget.ConstraintLayout>